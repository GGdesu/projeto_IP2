<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="475.0" prefWidth="507.0" stylesheets="@estilo.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ticTacThink.gui.RankController">
   <center>
      <TableView fx:id="tvRank" mouseTransparent="true" pickOnBounds="false" prefHeight="367.0" prefWidth="378.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="posCol" editable="false" prefWidth="75.0" sortable="false" text="Posição" />
          <TableColumn fx:id="nomeCol" editable="false" prefWidth="75.0" sortable="false" text="Nome" />
            <TableColumn fx:id="pontCol" editable="false" prefWidth="75.0" sortable="false" text="Pontuação" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <effect>
            <InnerShadow />
         </effect>
      </TableView>
   </center>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <buttons>
            <Button mnemonicParsing="false" onAction="#voltar" prefHeight="25.0" prefWidth="74.0" styleClass="botao-minusculo" text="Voltar" />
         </buttons>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets right="5.0" />
         </padding>
      </ButtonBar>
   </bottom>
</BorderPane>
